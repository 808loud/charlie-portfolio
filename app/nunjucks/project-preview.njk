
<article class="project row {{ project.slug }}" style="background-image: url('{{ site.media_directory }}{{ project.preview_img }}');">
  <h2 class="sr-only">{{ project.slug }}</h2>
  <div class="col-xs-12 project-inner">
    <div class="row">
      <div class="col-xs-12">
        <img class="" src="{{ site.media_directory }}{{ project.logo.src }}" width="{{ project.logo.width }}" height="{{ project.logo.height }}" alt="{{ project.button }}">
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <button class="btn btn-default" type="button" data-toggle="modal" data-target="#{{ project.slug }}">{{ project.button }}</button>
      </div>
    </div>
  </div>
</article>
