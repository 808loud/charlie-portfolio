
<article class="project row {{ project.slug }}" style="background-image: url('/media/{{ project.preview_img }}');">
  <div class="col-xs-12 project-inner">
    <div class="row">
      <div class="col-xs-12">
        <img class="" src="/media/{{ project.logo.src }}" width="{{ project.logo.width }}" height="{{ project.logo.height }}" alt="{{ project.button }}">
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <button class="btn btn-default" type="button" data-toggle="modal" data-target="#{{ project.slug }}">{{ project.button }}</button>
      </div>
    </div>
  </div>
</article>